{
  "name": "horrible-timer",
  "version": "1.0.0",
  "description": "A horrible Pomodoro timer",
  "files": [
    "source/cypress"
  ],
  "directories": {
    "test": "source/tests"
  },
  "scripts": {
    "start": "http-server source/src -p=5500",
    "watch": "watch 'npm run instrument' source/src/components source/src/scripts",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint --fix --fix-type layout source/",
    "instrument": "npx rimraf source/src/instrumented && npx nyc instrument --compact=false source/src source/src/instrumented",
    "cy:verify": "cypress verify",
    "cy:info": "cypress cache list"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jackie1342/cse110-w21-group13.git"
  },
  "author": "Kenneth Chen <efeichen@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/Jackie1342/cse110-w21-group13/issues"
  },
  "homepage": "https://github.com/Jackie1342/cse110-w21-group13#readme",
  "devDependencies": {
    "@babel/core": "^7.10.3",
    "@babel/plugin-proposal-class-properties": "^7.10.1",
    "@babel/preset-env": "^7.13.8",
    "@cypress/code-coverage": "^3.9.0",
    "@testing-library/jest-dom": "^5.11.9",
    "babel-jest": "^26.6.3",
    "babel-plugin-istanbul": "^6.0.0",
    "cypress": "^6.9.1",
    "eslint": "^7.19.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-plugin-cypress": "^2.11.2",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jest": "^24.1.3",
    "husky": "^4.3.8",
    "istanbul-instrumenter-loader": "^3.0.1",
    "istanbul-lib-coverage": "^3.0.0",
    "jest": "^26.6.3",
    "jest-localstorage-mock": "^2.4.7",
    "jsdoc": "^3.6.6",
    "lint-staged": "^10.5.4",
    "nyc": "^15.1.0"
  },
  "jest": {
    "verbose": true,
    "collectCoverage": true,
    "coverageDirectory": "source/coverage/jest",
    "testEnvironment": "jest-environment-jsdom-sixteen",
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "resetMocks": false,
    "setupFiles": [
      "jest-localstorage-mock"
    ],
    "modulePathIgnorePatterns": [
      "/cypress/"
    ]
  },
  "dependencies": {
    "http-server": "^0.12.3",
    "jest-environment-jsdom-sixteen": "^1.0.3",
    "regenerator-runtime": "^0.13.7",
    "rimraf": "^3.0.2",
    "watch": "^1.0.2"
  },
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-proposal-class-properties"
    ]
  },
  "nyc": {
    "report-dir": "source/coverage/cypress",
    "reporter": [
      "text",
      "json",
      "lcov"
    ]
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint && npm run instrument"
    }
  }
}
